# Variables d'environnement - Portfolio Liquid Glass

# Configuration site
NEXT_PUBLIC_SITE_URL=http://localhost:3000
NEXT_PUBLIC_SITE_NAME="Portfolio Liquid Glass"

# APIs externes
NEXT_PUBLIC_COINGECKO_API_KEY=your_coingecko_api_key
NEXT_PUBLIC_OPENSEA_API_KEY=your_opensea_api_key
NEXT_PUBLIC_SETMORE_API_KEY=your_setmore_api_key

# Analytics
NEXT_PUBLIC_GA_ID=your_google_analytics_id
NEXT_PUBLIC_MIXPANEL_TOKEN=your_mixpanel_token

# Features flags
NEXT_PUBLIC_ENABLE_AR=true
NEXT_PUBLIC_ENABLE_VOICE=true
NEXT_PUBLIC_ENABLE_CRYPTO=true
NEXT_PUBLIC_ENABLE_AI=true

# Base de données
DATABASE_URL=your_database_url
REDIS_URL=your_redis_url

# Authentification
JWT_SECRET=your_jwt_secret
NEXTAUTH_SECRET=your_nextauth_secret

# Déploiement
VERCEL_URL=your_vercel_url
